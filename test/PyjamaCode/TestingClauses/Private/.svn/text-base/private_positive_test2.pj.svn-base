package PyjamaCode.TestingClauses.Private;

import pj.Pyjama;

public class private_positive_test2{
	/**
	 *  a private copy is an uninitialized variable by the same name and same type as the original variable;
	 *  i in parallel region is a new instance of i;
	 * */
	public int parallel_for_private(int threadNumber){
		Pyjama.omp_set_num_threads(threadNumber);
		
		int shijun=-1;
		//#omp parallel  
		{
			//#omp for schedule(static) private(shijun)
			for (int j = 0; j < 10; j++) {
				shijun++;
			}			
		}
		return shijun;
	}
	
}